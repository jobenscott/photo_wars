

<h1> Best Images </h1>
<%= link_to "Post a new pic!", new_image_path %>
<% @images.each do |img| %>
  <h2><%= img.title %></h2>
  <h6><%= img.url %></h6>
  <h3><%= img.vote %></h3>
  <p><%= img.description %></p>
  <p><%= img.user_id %></p>
  <%= link_to "This image", image_path(img) %>
  <%= link_to "Edit image", edit_image_path(img) %>
  <%= link_to "KILL!!!", img, method: :delete %>
<% end %>
